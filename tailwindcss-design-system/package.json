{
  "name": "tailwind-ds",
  "license": "MIT",
  "scripts": {
    "build:style": "postcss ./components/styles/index.css --config ./components/styles/postcss.config.js -o dist/index.css",
    "build:style:prod": "NODE_ENV=production yarn build:style",
    "storybook": "npm-run-all --parallel 'build:style -w' start-storybook",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "yarn build:style:prod && build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^17.0.1"
  }
}
